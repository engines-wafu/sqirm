$(document).ready(function() {

  /**
   * call the data.php file to fetch the result from db table.
   */
  $.ajax({
    url : "http://www.blackcataerospace.com/api/data.php",
    type : "GET",
    success : function(data){
      console.log(data);

    },
    error : function(data) {
      console.log(data);
    }
  });

});

